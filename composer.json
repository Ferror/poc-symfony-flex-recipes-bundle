{
    "name": "ferror/recipes-bundle",
    "description": "The way to test Symfony Flex Serverless",
    "type": "symfony-bundle",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "symfony/framework-bundle": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ferror\\RecipesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ferror\\RecipesBundle\\": "tests/"
        }
    }
}
